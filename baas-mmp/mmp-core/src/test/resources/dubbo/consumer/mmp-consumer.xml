<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
			 http://www.springframework.org/schema/context
    	   http://www.springframework.org/schema/context/spring-context-3.0.xsd
			http://code.alibabatech.com/schema/dubbo
			http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
    <dubbo:application name="runner_mmp_service"/>
   	<dubbo:registry protocol="zookeeper" address="127.0.0.1:2181" />
	<dubbo:protocol name="dubbo" port="40880" />
    <dubbo:consumer retries="2"/>
    <dubbo:monitor protocol="registry" />

<!--     <dubbo:annotation package="omc.test.manage.impl"/> -->
<!--     <dubbo:reference interface="com.ai.runner.center.mmp.api.manager.interfaces.MMPManager" id="mmpManager"/> -->
    <dubbo:reference interface="com.ai.runner.center.mmp.api.manager.interfaces.SMSServices" id="sMSServices"/>
</beans>